<!-- Checkpoint -->
<sprite id="25" name="Checkpoint Flag" notes="When Mario collides with this flagpole, the game will set Mario's spawn point to it. The flag will turn into the color of the player (Mario = red, Luigi = green, YToad = yellow, BToad = blue) and will give the player who collides with it a mushroom. If a player dies in the same level, then the game will respawn all players at the flagpole. If someone decides to play another level or completes the level with the flagpole active, then the game will reset all memory in the flagpole and reset the spawn point location.">
	<value nybble="5" title="Useless(?) Value 5" comment="This field is no longer even referenced in the code."/>
	<value nybble="7-8" title="Entrance ID" comment="The entrance when the player will be spawned if he dies after reaching the midway flag."/>
	<checkbox nybble="9" title="Can bounce on Bouncy Clouds"/>
	<list nybble="10" title="Appears">
		<entry value="0">Always</entry>
		<entry value="1">Only if midway flag is not yet touched</entry>
		<entry value="2">Only if midway flag is already touched</entry>
	</list>
	<checkbox nybble="11" title="Unknown Checkbox 11"/>
	<checkbox nybble="11" title="Affected by gravity" mask="2"/>
	<checkbox nybble="12" title="Second checkpoint (Uses Time Limit 3)" comment="Set this if this is the second midway point in the level. If unchecked, Time Limit 2 will be used instead."/>
	<checkbox nybble="20" title="Spawn on ground"/>
    <list nybble="21" title="Orientation">
        <entry value="0">Upright</entry>
        <entry value="1">Right</entry>
        <entry value="2">Upside Down</entry>
        <entry value="3">Left</entry>
    </list>
    <list nybble="22" title="Powerup" comment="This only applies to the player when small.">
        <entry value="0">Big</entry>
        <entry value="1">Fire</entry>
        <entry value="2">Mini</entry>
        <entry value="3">Propeller</entry>
        <entry value="4">Penguin</entry>
        <entry value="5">Ice</entry>
        <entry value="6">Acorn</entry>
        <entry value="7">P-Acorn</entry>
        <entry value="8">None</entry>
    </list>
</sprite>

